#!/usr/bin/perl

use 5.8.1;
use strict;
use warnings 'all';

my $line = <>;
$line =~ m/(?:(.)(?!.{0,12}\1)(.)(?!.{0,11}\2)(.)(?!.{0,10}\3)(.)(?!.{0,9}\4)(.)(?!.{0,8}\5)(.)(?!.{0,7}\6)(.)(?!.{0,6}\7)(.)(?!.{0,5}\8)(.)(?!.{0,4}\9)(.)(?!.{0,3}\10)(.)(?!.{0,2}\11)(.)(?!.?\12)(.)(?!\13).)/g;
print pos($line),"\n";


